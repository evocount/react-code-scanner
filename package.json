{
	"name": "react-code-scanner",
	"version": "0.0.2",
	"description": "A react module for scanning codes like barcodes and QR codes.",
	"main": "build/index.js",
	"scripts": {
		"build:development": "cross-env TS_NODE_PROJECT=\"webpack.tsconfig.json\" npx webpack --config webpack/development.js",
		"build:production": "npm run clean && cross-env TS_NODE_PROJECT=\"webpack.tsconfig.json\" npx webpack --config webpack/production.js",
		"clean": "rm -rf build",
		"test": "echo \"Error: no test specified\" && exit 1",
		"prepack": "npm run clean && npm run build:production"
	},
	"keywords": [
		"Barcode",
		"QR-Code",
		"React"
	],
	"author": "EvoCount GmbH",
	"license": "MIT",
	"devDependencies": {
		"@types/react": "^17.0.5",
		"@types/w3c-image-capture": "^1.0.2",
		"cross-env": "^7.0.3",
		"ts-loader": "^9.1.2",
		"typescript": "^4.2.4",
		"copy-webpack-plugin": "^8.1.1",
		"webpack": "^5.36.2",
		"webpack-cli": "^4.7.0"
	},
	"peerDependencies": {
		"react": "^17.0.2"
	},
	"dependencies": {
		"@evocount/signal": "0.0.0",
		"@zxing/browser": "0.0.9",
		"@zxing/library": "^0.18.5",
		"image-capture": "^0.4.0"
	}
}
